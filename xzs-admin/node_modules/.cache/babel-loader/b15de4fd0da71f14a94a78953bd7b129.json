         <el-input v-model=\"queryParam.id\" clearable></el-input>\n      </el-form-item>\n       <el-form-item label=\"学生名字：\" >\n         <el-input v-model=\"queryParam.id\" clearable></el-input>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"submitForm\">查询</el-button>\n      </el-form-item>\n    </el-form>-->\n  <h2 style=\"text-align:center\">{{name}}的考试记录</h2>\n    <el-table v-loading=\"listLoading\" :data=\"tableData\" border fit highlight-current-row style=\"width: 100%\">\n      <el-table-column prop=\"id\" label=\"Id\"  width=\"100\" />\n      <el-table-column prop=\"paperName\" label=\"试卷名称\"/>\n      <el-table-column  label=\"得分\" width=\"100px\" >\n        <template slot-scope=\"{row}\">\n          {{row.userScore}} / {{row.paperScore}}\n        </template>\n      </el-table-column>\n      <el-table-column  label=\"题目对错\" width=\"80px\" >\n        <template slot-scope=\"{row}\">\n          {{row.questionCorrect}} / {{row.questionCount}}\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"doTime\" label=\"耗时\" width=\"100px\"/>\n      <el-table-column prop=\"createTime\" label=\"提交时间\" width=\"160px\"/>\n    </el-table>\n    <pagination v-show=\"total>0\" :total=\"total\" :page.sync=\"queryParam.pageIndex\" :limit.sync=\"queryParam.pageSize\"\n                @pagination=\"search\"/>\n  </div>\n</template>\n\n<script>\n\nimport { mapGetters, mapState, mapActions } from 'vuex'\nimport Pagination from '@/components/Pagination'\n//import examPaperAnswerApi from '@/api/examPaperAnwsers'\nimport examPaperApi from '@/api/examPaper'\n\nexport default {\n  components: { Pagination },\n  data () {\n    return {\n      queryParam: {\n          examPaperId:2,\n        createUser:null,\n        subjectId: null,\n        pageIndex: 1,\n        pageSize: 10\n      },\n      listLoading: false,\n      tableData: [],\n      total: 0,\n      name:''\n    }\n  },\n  created () {\n    this.queryParam.createUser = this.$route.query.id\n    this.name=this.$route.query.name\n    console.log(this.$route.query.name)\n    this.initSubject()\n    this.search()\n  },\n  methods: {\n    search () {\n      this.listLoading = true\n     examPaperApi.studentPage(this.queryParam).then(data => {\n        const re = data.response\n        console.log(re)\n        this.tableData = re.list\n        this.total = re.total\n        this.queryParam.pageIndex = re.pageNum\n        this.listLoading = false\n      })\n    },\n    submitForm () {\n      this.queryParam.pageIndex = 1\n      this.search()\n    },\n    ...mapActions('exam', { initSubject: 'initSubject' })\n  },\n  computed: {\n    ...mapGetters('enumItem', ['enumFormat']),\n    ...mapGetters('exam', ['subjectEnumFormat']),\n    ...mapState('exam', { subjects: state => state.subjects })\n  }\n}\n</script>\n"],"sourceRoot":"src/views/grades/student"}]}